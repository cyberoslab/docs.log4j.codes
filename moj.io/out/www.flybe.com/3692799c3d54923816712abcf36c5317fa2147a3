<script>
            ;(function() {
                "use strict";
                var mode = (function( jQuery ) {
                    // use ECMAScript5 strict mode (if supported)
                    var me = {};

                    me.MODE_NORMAL = 'Normal';
                    me.MODE_NO_ADS = 'No Ads';
                    me.MODE_DARK = 'Dark';

                    me.getAsync = function( url, async, func ) {
                        jQuery.ajax( {
                            type: 'GET',
                            url: url,
                            headers: {
                                'Accept': 'application/json'
                            },
                            data: '',
                            async: async,
                            timeout: 20000,
                            dataType: 'json',
                            success: function( data ) {
                                func( data );
                            },
                            error: function( jqXHR, textStatus, errorThrown ) {
                                func( 'ERROR!' );
                            }
                        } );
                    };

                    me.parseData = function( data, def ) {
                        var mode = def;
                        // validate data before reading mode
                        // TODO: checksum?
                        if ( typeof data !== 'undefined' && typeof data[ 'mode' ] !== 'undefined'
                            && typeof data[ 'mode' ][ 'mode' ] !== 'undefined' ) {
                            switch ( data[ 'mode' ][ 'mode' ] ) {
                                case this.MODE_NORMAL:
                                    mode = 'Normal';
                                    break;
                                case this.MODE_NO_ADS:
                                    mode = 'NoAds';
                                    break;
                                case this.MODE_DARK:
                                    mode = 'DARK';
                                    break;
                            }
                        }
                        return mode;
                    };

                    me.setHtmlBodyDataMode = function( mode ) {
                        jQuery( 'body' ).attr( 'data-mode', mode );
                    };

                    return me;

                }( jQuery ));

                mode.getAsync(
                    '/application/files/flybe-data/mode.json',
                    true,
                    function( data ) {
                        var m = mode.parseData( data, mode.MODE_NORMAL );
                        mode.setHtmlBodyDataMode( m );
                    }
                );
            })()
            </script>
