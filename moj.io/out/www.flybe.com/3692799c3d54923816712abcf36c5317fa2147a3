        //login handler for navigation button
        function login() {       
            Flybe.userService.login();
            return true;
        }

        //check login status and assign text to button
        if(Flybe.userService.isLoggedIn()){ 
            $('#sign-in-button').text(i18next.t("page_header.my_account.TEXT"));
        } else {
            $('#sign-in-button').text(i18next.t("page_header.my_account.LOGIN"));
        }        
